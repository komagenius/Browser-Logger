<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>UCIWebSearchLogger@ics.uci.edu</em:id>
    <em:type>2</em:type>
    <em:name>UCI Web-Search Logger</em:name>
    <em:version>1.0</em:version>
    <em:creator>Phitchayaphong Tantikul</em:creator>
    <em:contributor>Rosalva Gallado</em:contributor>
    <em:description>&quot;UCI Web-Search Logger&quot; Chrome Extension will record the queries you enter in Google, StackOverflow, and Bing. It will also record the url of the search results you visit. It will ask you to submit your recorded searches to the researchers everyday. You can stop logging your searches at any time or you can delete searches you do not want to report before submitting your recorded searches.</em:description>
    <em:aboutURL>chrome://UCIWebSearchLogger/content/about.xul</em:aboutURL>
    <em:optionsURL>chrome://UCIWebSearchLogger/content/options.xul</em:optionsURL>
    <em:iconURL>chrome://UCIWebSearchLogger/skin/logger_icon.png</em:iconURL>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>3.5</em:minVersion>
        <em:maxVersion>4.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
